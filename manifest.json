{
	"manifest_version": 2,
	"name": "TwitchEmotes",
	"version": "1.0.5",
	"description": "Displays select Twitch, BetterTTV and FrankerFaceZ emotes on all sites.",
	"icons": {
		"128": "icon.png"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"all_frames": true,
		"match_about_blank": true,
		"js": ["jquery-3.2.1.min.js", "replacer.js"]
	}]
}